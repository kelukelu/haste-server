{
  "name": "haste",
  "version": "0.2.2",
  "private": true,
  "description": "Private Pastebin Server - Wrapper for CozyCloud",
  "keywords": [
    "paste",
    "pastebin",
    "cozycloud"
  ],
  "author": {
    "name": "John Crepezzi",
    "email": "john.crepezzi@gmail.com",
    "url": "http://seejohncode.com/"
  },
  "main": "haste",
  "dependencies": {
    "async": "^0.9.0",
    "basic-auth": "^1.0.0",
    "busboy": "0.2.8",
    "connect": "3.1.1",
    "connect-route": "git://github.com/neandrake/connect-route.git#bc44be0fac3e7ccda51221c07b428a5f3594bd06",
    "cozydb": "0.0.11",
    "mime": "1.2.11",
    "printit": "^0.1.6",
    "spirc": "0.1.4",
    "st": "0.5.1",
    "uglify-js": "2.4.15",
    "winston": "0.7.3"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*"
  },
  "bundledDependencies": [],
  "engines": {
    "node": "0.10.31+",
    "npm": "1.4.24+"
  },
  "bin": {
    "haste-server": "./server.js"
  },
  "files": [
    "server.js",
    "lib",
    "static"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "postinstall": "cp config-dist.js config.js && chmod 666 config.js",
    "start": "node server.js",
    "test": "mocha -r should spec/*"
  },
  "cozy-permissions": {
    "Haste": {
      "description": "Store your snippets into Cozy data system"
    },
    "HasteSettings": {
      "description": "Application settings"
    }
  },
  "cozy-displayName": "Haste",
  "icon-path": "haste.png"
}
